<%- include('partials/header') %>

<div class="container">
    <div class="row" id="yourp">Your Projects</div>
  <div class="row row-cols-1 row-cols-md-3 g-4">
  
   <%   for(var i=0; i < projects.length ;i++) {%>
  
  <div class="col">
    <div class="card" style="width: 18rem;">
      <div class="card-body">
        <h5 class="card-title"><%-   projects[i].pname%> </h5>
          <p class="card-text"><%-   projects[i].desc%></p>
          <p1 class="time"><%-  projects[i].time%> <p1 class="Status">Status: <%- projects[i].status %> </p>
        <a href="/projects/<%- projects[i].id %> " class="card-link">Open Project</a>
        <a href="#" class="card-link">Delete Project</a>
      </div>
    </div>
  </div>
    <% } %> 
  </div>
  </div>

<%- include('partials/footer') %>
